#include<stdio.h>
#include<math.h>
int main()
{
int a,b,c;
printf("Enter 3 sides of Triangle");
scanf("%d%d%d,&a,&b,&c");
if (a+b>c && a+c>c && b+c>a)
{
    printf("Triangle is possible");
    if(a==b && b==c)
    { printf("Triangle is equilateral");}
    else if (a==b)
    { printf("Triangle is Isoceles");}
    else if (b==c)
    { printf("Triangle is Isoceles");}
    else if (a==c)
    { printf("Triangle is Isoceles");}
    else
    { printf("Triangle is scalene");}
}
else 
{ printf("Triangle is not possible");}
return 0;
}
